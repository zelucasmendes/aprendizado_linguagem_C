/*Criar um algoritmo que deixe entrar com dois números e 
imprimir o quadrado do menor e o cubo do maior número.
*/

#include <stdio.h>
#include <math.h>
main()

{float num1,num2,quad,cubo;

	printf("Digite dois numeros:\n");
	scanf("%f%f",&num1,&num2);
	
	if (num1>num2)
	{
	quad = pow(num2,2);
	cubo = pow (num1,3);
	printf("O quadrado do menor e %.2f e o cubo do maior e %.2f",quad,cubo);	   	
	}
	else
	{
	quad = pow(num1,2);
	cubo = pow(num2,3);
	printf("O quadrado do menor e %.2f e o cubo do maior e %.2f",quad,cubo);	
	}	
}
